<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="System.Ware.Ware" table="ware" >
		<id name="wareId" type="integer" >
			<column name="wareId" />
			<generator class="native"></generator>
		</id>
		<property name="wareName" type="string" column="wareName" />
		<property name="warePrice" type="string" column="warePrice" />
		<property name="description" type="string" column="description" />
		<property name="wareKind" type="string" column="wareKind" />
		<property name="warekey" type="string" column="warekey" />
		<property name="wareimg" type="string" column="wareimg" />
		<property name="status" type="int" column="status" ></property>
	</class>
</hibernate-mapping>