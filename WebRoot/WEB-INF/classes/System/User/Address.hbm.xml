<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="System.User.Address" table="address" >
		<id name="addressId" type="integer" >
		</id>
		
		<many-to-one name="user" class="System.User.User" column="userId" ></many-to-one>
		
		<property name="address" type="string" column="address" />
		<property name="addressee" type="string" column="addressee" />
		<property name="addtel" type="string" column="addtel" />
		
	</class>
</hibernate-mapping>
